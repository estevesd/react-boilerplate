{
  "name": "react-bolt",
  "version": "1.0.0",
  "description": "âš¡ The most simple & robust boilerplate for your React projects.",
  "keywords": [
    "react",
    "redux",
    "webpack",
    "jest",
    "styled-components",
    "starter",
    "boilerplate"
  ],
  "author": "Leonardo Maldonado <leonardomso11@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/estevesd/react-bolt.git"
  },
  "bugs": {
    "url": "https://github.com/estevesd/react-bolt/issues"
  },
  "homepage": "https://github.com/estevesd/react-bolt#readme",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "build": "cross-env NODE_ENV=production webpack",
    "lint": "eslint ./src/**/**.js",
    "lint:fix": "eslint ./src/**/**.js --fix",
    "test": "jest",
    "test:watch": "yarn run test --watch",
    "test:cover": "yarn run test --coverage",
    "cypress:open": "cypress open",
    "precommit": "yarn run lint && yarn test",
    "prepush": "yarn run lint"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/tests/jest.config"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
